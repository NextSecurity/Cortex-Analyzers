{
    "name": "IntezerCommunity",
    "version": "1.0",
    "author": "Efi Jeremiah, NextSecurity",
    "url": "https://github.com/nextsecurity/Cortex-Analyzers-Modified",
    "license": "AGPL-v3",
    "description": "Analyze a possible malicious file with Intezer Analyzer",
    "dataTypeList": ["file"],
    "baseConfig": "IntezerCommunity",
    "command": "IntezerCommunity/intezer_community.py",
    "configurationItems": [
      {
        "name": "key",
        "description": "API key for Intezer",
        "type": "string",
        "multi": false,
        "required": true
      }
    ],
    "config": {
        "check_tlp": true,
        "max_tlp": 2,
        "auto_extract": true
    }
}